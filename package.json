{
  "name": "openredaction-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run copy-openredaction && npm run docs:export && next build",
    "copy-openredaction": "mkdir -p public/lib && cp node_modules/openredaction/dist/index.js public/lib/openredaction.js",
    "start": "next start",
    "lint": "next lint",
    "docs:export": "cd docs && npm ci && npm run build && rm -rf ../public/docs && mkdir -p ../public && cp -R out ../public/docs"
  },
  "dependencies": {
    "@stripe/react-stripe-js": "^2.4.0",
    "@stripe/stripe-js": "^2.4.0",
    "@vercel/analytics": "^1.6.1",
    "botid": "^1.5.10",
    "clsx": "^2.1.0",
    "lucide-react": "^0.344.0",
    "next": "^14.2.0",
    "nodemailer": "^7.0.12",
    "openredaction": "^1.0.8",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "resend": "^6.4.2",
    "stripe": "^14.21.0",
    "tailwind-merge": "^2.2.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/nodemailer": "^7.0.4",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.21.0",
    "typescript": "^5.4.0"
  }
}
